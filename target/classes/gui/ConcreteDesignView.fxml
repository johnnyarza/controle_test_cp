<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox minWidth="417.0" prefHeight="347.0" prefWidth="896.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ConcreteDesignViewController">
   <children>
      <Label text="Lista de Disenõs">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <HBox alignment="CENTER_LEFT" minHeight="35.0" minWidth="166.0" prefHeight="42.0" prefWidth="496.0" spacing="5.0">
         <children>
            <Button fx:id="btNew" mnemonicParsing="false" onAction="#onBtNewAction" text="Nuevo" />
            <Button fx:id="btEdit" mnemonicParsing="false" onAction="#onBtEditAction" text="Editar" />
            <Button fx:id="btDelete" mnemonicParsing="false" onAction="#onBtDeleteAction" text="Apagar" />
            <Button fx:id="btPrint" mnemonicParsing="false" onAction="#onBtPrintAction" text="Imprimir" />
         </children>
         <padding>
            <Insets left="5.0" />
         </padding>
      </HBox>
      <TableView fx:id="tableViewConcreteDesing" minWidth="166.0" prefHeight="387.0" prefWidth="421.0">
        <columns>
          <TableColumn fx:id="tableColumnId" minWidth="30.0" prefWidth="30.0" text="iD" />
            <TableColumn fx:id="tableColumnName" maxWidth="500.0" minWidth="65.0" prefWidth="65.0" text="Nombre" />
            <TableColumn fx:id="tableColumnSlump" prefWidth="75.0" text="Slump" />
          <TableColumn fx:id="tableColumnDesc" maxWidth="1.7976931348623157E308" minWidth="110.0" prefWidth="800.0" text="Descripción" />
        </columns>
      </TableView>
   </children>
</VBox>
