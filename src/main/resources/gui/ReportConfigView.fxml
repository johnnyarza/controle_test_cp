<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="347.0" prefWidth="496.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ReportConfigViewController">
   <children>
      <Label text="Configuraciones de Reporte">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
         <VBox.margin>
            <Insets left="8.0" />
         </VBox.margin>
      </Label>
      <HBox alignment="CENTER_LEFT" spacing="5.0" style="-fx-background-color: #e0dede;">
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <children>
            <Button fx:id="btEditAdmin" mnemonicParsing="false" onAction="#onBtEditAdmin" styleClass="safe-button" text="Editar adm." />
            <Button fx:id="btNewAdmin" mnemonicParsing="false" onAction="#onBtNewAdmin" styleClass="safe-button" text="Nuevo adm." />
            <Button fx:id="btDeleteAdmin" mnemonicParsing="false" onAction="#onBtDeleteAdmin" styleClass="dangerous-button" text="Apagar adm." />
         </children>
         <padding>
            <Insets bottom="8.0" left="8.0" top="8.0" />
         </padding>
      </HBox>
      <Label prefHeight="18.0" prefWidth="495.0" text="Imagen del Reporte">
         <VBox.margin>
            <Insets bottom="8.0" top="8.0" />
         </VBox.margin>
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
         <padding>
            <Insets left="8.0" />
         </padding>
      </Label>
      <HBox prefHeight="145.0" prefWidth="496.0">
         <children>
            <VBox prefHeight="146.0" prefWidth="248.0" spacing="5.0">
               <children>
                  <HBox alignment="CENTER_LEFT" minHeight="30.0" prefHeight="30.0" prefWidth="226.0">
                     <children>
                        <Button fx:id="btLogo" contentDisplay="CENTER" graphicTextGap="0.0" minHeight="15.0" mnemonicParsing="false" onAction="#onBtLogoAction" prefHeight="25.0" prefWidth="25.0" styleClass="safe-button">
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                        </Button>
                     </children>
                  </HBox>
                  <ImageView fx:id="imgLogo" fitHeight="87.0" fitWidth="238.0" pickOnBounds="true" preserveRatio="true" />
               </children>
               <padding>
                  <Insets left="8.0" />
               </padding>
            </VBox>
            <VBox prefHeight="208.0" prefWidth="248.0" spacing="5.0">
               <children>
                  <HBox alignment="CENTER_LEFT" minHeight="30.0" prefHeight="30.0" prefWidth="226.0">
                     <children>
                        <Button fx:id="btCarimbo" contentDisplay="CENTER" graphicTextGap="0.0" minHeight="15.0" mnemonicParsing="false" onAction="#onBtCarimboAction" prefHeight="25.0" prefWidth="25.0" styleClass="safe-button" />
                     </children>
                  </HBox>
                  <ImageView fx:id="imgCarimbo" fitHeight="87.0" fitWidth="237.0" pickOnBounds="true" preserveRatio="true" />
               </children>
               <padding>
                  <Insets left="8.0" />
               </padding>
            </VBox>
         </children>
      </HBox>
   </children>
</VBox>
