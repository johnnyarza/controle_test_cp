<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="347.0" prefWidth="496.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ReportConfigViewController">
   <children>
      <Label text="Configuraciones de Reporte">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
         <VBox.margin>
            <Insets left="5.0" top="5.0" />
         </VBox.margin>
      </Label>
      <HBox alignment="CENTER_LEFT" prefHeight="39.0" prefWidth="496.0" spacing="5.0" style="-fx-background-color: #e0dede;">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
      <HBox prefHeight="145.0" prefWidth="496.0">
         <children>
            <TitledPane prefHeight="235.0" prefWidth="496.0" text="Seleccionar Logotipo">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
               <content>
                  <VBox prefHeight="168.0" prefWidth="246.0">
                     <children>
                        <HBox alignment="CENTER_LEFT" minHeight="30.0" prefHeight="30.0" prefWidth="226.0">
                           <children>
                              <Button fx:id="btLogo" contentDisplay="CENTER" graphicTextGap="0.0" minHeight="15.0" mnemonicParsing="false" onAction="#onBtLogoAction" prefHeight="25.0" prefWidth="25.0">
                                 <HBox.margin>
                                    <Insets left="5.0" />
                                 </HBox.margin>
                              </Button>
                           </children>
                        </HBox>
                        <ImageView fx:id="imgLogo" fitHeight="60.0" fitWidth="225.0" pickOnBounds="true" preserveRatio="true" />
                     </children>
                  </VBox>
               </content>
            </TitledPane>
            <TitledPane prefHeight="228.0" prefWidth="496.0" text="Seleccionar Carimbo">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
               <HBox.margin>
                  <Insets right="1.0" />
               </HBox.margin>
               <content>
                  <VBox prefHeight="208.0" prefWidth="316.0">
                     <children>
                        <HBox alignment="CENTER_LEFT" minHeight="30.0" prefHeight="30.0" prefWidth="226.0">
                           <children>
                              <Button fx:id="btCarimbo" contentDisplay="CENTER" graphicTextGap="0.0" minHeight="15.0" mnemonicParsing="false" onAction="#onBtCarimboAction" prefHeight="25.0" prefWidth="25.0" />
                           </children>
                        </HBox>
                        <ImageView fx:id="imgCarimbo" fitHeight="60.0" fitWidth="224.0" pickOnBounds="true" preserveRatio="true" />
                     </children>
                  </VBox>
               </content>
            </TitledPane>
         </children>
      </HBox>
   </children>
</VBox>
